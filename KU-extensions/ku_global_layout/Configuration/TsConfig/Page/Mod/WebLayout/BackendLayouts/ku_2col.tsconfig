#
# BACKENDLAYOUT: KU two columns
#
mod {
    web_layout {
        BackendLayouts {
            ku_2col {
                title = LLL:EXT:ku_global_layout/Resources/Private/Language/locallang_be.xlf:backend_layout.2col
                config {
                    backend_layout {
                        colCount = 2
                        rowCount = 2
                        rows {
                            1 {
                                columns {
                                    1 {
                                        name = LLL:EXT:ku_global_layout/Resources/Private/Language/locallang_be.xlf:backend_layout.2col.left
                                        colPos = 18
                                    }
                                    2 {
                                        name = LLL:EXT:ku_global_layout/Resources/Private/Language/locallang_be.xlf:backend_layout.2col.right
                                        colPos = 19
                                    }
                                }
                            }
                        }
                        rows {
                            2 {
                                columns {
                                    1 {
                                        name = LLL:EXT:ku_global_layout/Resources/Private/Language/locallang_be.xlf:backend_layout.2col.center
                                        colPos = 20
                                        colspan = 12
                                    }
                                }
                            }
                        }
                    }
                }
                icon = EXT:ku_global_layout/Resources/Public/Icons/BackendLayouts/ku.svg
            }
        }
    }
}
