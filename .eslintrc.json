{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "globals": {
        "debounce": "readonly",
        "throttle": "readonly",
        "CKEDITOR": "readonly",
        "require": "readonly",
        "$":"writable"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}
